programa factorial_recursivo;
var int entrada;

funcion int factorial_recursivo(int i)
var int aux; 
{
    si(i == 0 || i == 1) entonces {
        regresa(1);
    }
    regresa(i*factorial_recursivo(i - 1));
}

principal() {
    escribe("Dime el valor");
    lee(entrada);
    escribe(factorial_recursivo(factorial_recursivo(entrada)));
}