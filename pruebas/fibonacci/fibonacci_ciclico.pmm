programa fibonacci_ciclico;
var int numero, fibo1, fibo2, i;

principal() {
    escribe("Introduce numero");
    lee(numero);

    fibo1 = 0;
    fibo2 = 1;
    escribe(fibo1);

    desde i = 1 hasta i >= numero hacer {
        escribe(fibo2);
        fibo2 = fibo1 + fibo2;
        fibo1 = fibo2 - fibo1;
    }
}